{
  "name": "sccdmaomao",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/sccdmaomao/sccdmaomao.git",
  "author": "guohao.yan <sccdmaomao@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "compile": "webpack",
    "test": "jest --passWithNoTests",
    "build:prod": "rm -rf ./dist & NODE_ENV=production PLATFORM=web webpack --progress --color",
    "build:prod:quiet": "NODE_ENV=production PLATFORM=web webpack --color",
    "build:prod:windows": "cross-env NODE_ENV=production PLATFORM=web webpack --color",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx  --fix",
    "deploy": "yarn build:prod:quiet && ./node_modules/.bin/git-directory-deploy --directory dist"
  },
  "lint-staged": {
    "*.{ts,tsx,css}": [
      "yarn lint",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/recommended": "^1.0.1",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "git-directory-deploy": "^1.5.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "yarn": "^1.22.19"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "typescript": "^4.8.4"
  }
}
