{
    "name": "sccdmaomao-personal-website",
    "version": "1.0.0",
    "description": "SPA developed in React & Typescript",
    "main": "index.tsx",
    "license": "MIT",
    "author": "Guohao Yan",
    "private": true,
    "engines": {
        "node": ">=9.2.0"
    },
    "scripts": {
        "dev": "webpack-dev-server --port 8001 --host 0.0.0.0 --mode development",
        "compile": "webpack",
        "test": "jest --passWithNoTests",
        "build:prod": "rm -rf ./dist & NODE_ENV=production PLATFORM=web webpack -p --progress --display-error-details --color",
        "build:prod:quiet": "NODE_ENV=production PLATFORM=web webpack -p --display-error-details --color",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "build:stats": "mkdir -p ./stats ; ./node_modules/.bin/webpack --config webpack.config.stats.js --json > stats/stats.json ; ./node_modules/.bin/analyze-bundle --bundle stats/bundle.js --stats stats/stats.json --dir ./stats",
        "optimize-svg": "find ./src/components/Icons -name \"*.svg\" -exec svgo {} \\;",
        "reactify-svg": "find ./src/components/Icons -name \"*.svg\" -execdir svgtoreact dir \\;",
        "svg": "yarn optimize-svg && yarn reactify-svg",
        "lint": "tslint --project tsconfig.json --config tslint.json",
        "deploy": "yarn build:prod:quiet && $(yarn bin)/git-directory-deploy --directory dist"
    },
    "lint-staged": {
        "*.{ts,tsx,css}": [
            "yarn lint",
            "prettier --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@blueprintjs/core": "^3.6.1",
        "@blueprintjs/datetime": "^3.1.0",
        "@blueprintjs/select": "^3.1.0",
        "@emotion/core": "^10.0.14",
        "@emotion/styled": "^10.0.14",
        "@types/react": "^16.0.27",
        "@types/react-dom": "^16.0.3",
        "@types/react-redux": "^7.1.1",
        "@types/react-router-dom": "^4.3.4",
        "@types/redux-form": "^8.1.4",
        "apollo-link-http": "^1.2.0",
        "babel-plugin-emotion": "^10.0.14",
        "connected-react-router": "^6.5.2",
        "emotion": "^10.0.14",
        "immutable": "^3.8.2",
        "lie": "^3.1.1",
        "lodash.get": "^4.4.2",
        "lodash.isempty": "^4.4.0",
        "lodash.replace": "^4.1.4",
        "moment": "^2.20.1",
        "react": "^16.3.2",
        "react-anchor-link-smooth-scroll": "^1.0.12",
        "react-apollo": "^2.0.1",
        "react-copy-to-clipboard": "^5.0.1",
        "react-dom": "^16.3.2",
        "react-intl": "^3.1.13",
        "react-redux": "^7.1.0",
        "react-redux-loading-bar": "^3.1.1",
        "react-reveal": "^1.2.1",
        "react-router-dom": "^5.0.1",
        "react-stickynode": "^2.0.1",
        "react-transition-group": "^4.3.0",
        "react-typing-animation": "^1.6.2",
        "redux": "^4.0.4",
        "redux-devtools-extension": "^2.13.8",
        "redux-form": "^8.2.4",
        "redux-typed-modules": "^2.2.1",
        "webpack-stats-plugin": "^0.2.1"
    },
    "devDependencies": {
        "@babel/polyfill": "^7.4.4",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^6.1.0",
        "@storybook/addon-links": "^3.2.17",
        "@storybook/react": "^3.2.17",
        "@types/jest": "^22.0.0",
        "@types/node": "^9.3.0",
        "@types/react-intl": "^2.3.18",
        "@types/storybook__react": "^3.0.5",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^22.0.3",
        "babel-loader": "^7.1.2",
        "babel-preset-react": "^6.24.1",
        "css-loader": "^0.28.7",
        "electrode-bundle-analyzer": "^1.0.0",
        "file-loader": "^1.1.5",
        "git-directory-deploy": "^1.5.1",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^3.0.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.9.0",
        "less-loader": "^4.0.5",
        "lint-staged": "^7.0.2",
        "npm-run-all": "^4.1.2",
        "prettier": "^1.8.2",
        "raf": "^3.4.0",
        "source-map-loader": "^0.2.3",
        "style-loader": "^0.20.1",
        "stylelint": "^9.2.0",
        "stylelint-config-standard": "^18.0.0",
        "svg-to-react-cli": "^1.5.0",
        "svgo": "^1.0.3",
        "ts-jest": "^24.0.2",
        "ts-loader": "^4.1.0",
        "tslint": "^5.8.0",
        "tslint-config-prettier": "^1.6.0",
        "tslint-plugin-prettier": "^1.3.0",
        "tslint-react": "^3.2.0",
        "typescript": "^2.6.2",
        "webpack": "^4.4.1",
        "webpack-cli": "^3.3.6",
        "webpack-dev-server": "^3.1.1",
        "yarn": "^1.3.2"
    }
}
