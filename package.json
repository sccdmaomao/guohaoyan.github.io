  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "compile": "webpack",
    "test": "jest --passWithNoTests",
    "build:prod": "rm -rf ./dist & NODE_ENV=production PLATFORM=web webpack --progress --color",
    "build:prod:quiet": "NODE_ENV=production PLATFORM=web webpack --color",
    "build:prod:windows": "cross-env NODE_ENV=production PLATFORM=web webpack --color",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx  --fix",
    "deploy": "yarn build:prod:quiet && ./node_modules/.bin/git-directory-deploy --directory dist"
  },
  "lint-staged": {
    "*.{ts,tsx,css}": [
      "yarn lint",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.4",
